# Arduino
Code for the Arduino
void loop() {
  digitalWrite(13, HIGH);   
  digitalWrite(11, HIGH); 
  digitalWrite(9, HIGH);
  digitalWrite(7, HIGH);
  digitalWrite(5, HIGH);  
  delay(1000);              
  digitalWrite(13, LOW);              
  digitalWrite(11, LOW);
  digitalWrite(9, LOW);         
  digitalWrite(7, LOW);   
  digitalWrite(5, LOW);
}
